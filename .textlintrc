{
  "plugins": [
    "jser-markdown"
  ],
  "filters": {
    "comments": true,
    "node-types": {
      "nodeTypes": [
        "Link"
      ]
    }
  },
  "rules": {
    "proofdict": {
      "autoUpdate": true
    },
    "no-start-duplicated-conjunction": {
      "interval": 2
    },
    "no-doubled-joshi": true
  }
}
